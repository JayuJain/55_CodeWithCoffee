<%- include("partials/header") -%>


    <div class="mainDiv">
        <div class="row">
            <div class="col-4">
                <!-- <nav id="navbar-example3" class="h-100 flex-column align-items-stretch pe-4 border-end"> -->

                <nav class="nav nav-pills flex-column">
                    <img class="img" src="/img/img.jpg" alt="Error">
                    <div class="username">
                        <!-- <h3 class="name" >
                    <h3 class="nametext" >Jayu Jain</h3><hr>
                </h3> -->
                        <div class="nametext">
                            <div class="row">
                                <div class="col-md-12 text-center">
                                    <h3 class="name"> Jayu Jain</h3>
                                    <h5 class="email">jayujain@gmail.com</h5>
                                </div>
                            </div>
                        </div>

                    </div>
                </nav>
                </nav>
                <div class="vl"></div>
            </div>

            <div>
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">Name</th>
                            <th scope="col">Email</th>

                        </tr>
                    </thead>
                    <tbody>
                        <% if(data.length){ for(var i=0; i< data.length; i++) {%>
                            <tr>
                                <th scope="row">
                                    <%= (i+1) %>
                                </th>
                                <td>
                                    <%= data[i].name%>
                                </td>
                                <td>
                                    <%= data[i].marks%>
                                </td>
                            </tr>
                            <% } }else{ %>
                                <tr>
                                    <td colspan="3">No user</td>
                                </tr>
                                <% } %>

                    </tbody>
                </table>
            </div>
        </div>

        <div class="col-8">
            <div id="container"></div>
            <div class="btns">
                <div class="opt">
                    <img src="/img/create.png" alt="Create Test"><br>
                    <button class="btn">Create Test</button>
                </div>

                <div class="opt">
                    <img src="/img/report.png" alt="Student Report"><br>
                    <button class="btn">Student Report</button>
                </div>

                <div class="opt">
                    <img src="/img/details.png" alt="Student Details"><br>
                    <button class="btn">Student Details</button>
                </div>


            </div>
        </div>
    </div>


    <script src="https://cdn.anychart.com/js/8.0.1/anychart-core.min.js"></script>
    <script src="https://cdn.anychart.com/js/8.0.1/anychart-pie.min.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"
